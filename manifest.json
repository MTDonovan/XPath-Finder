{
  "manifest_version": 3,
  "name": "XPath Finder",
  "version": "1.0",
  "description": "A Chrome extension to find and parse xpath queries from a webpage.",
  "permissions": [
    "activeTab",
    "storage",
    "contextMenus"
  ],
  "devtools_page": "devtools.html",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
